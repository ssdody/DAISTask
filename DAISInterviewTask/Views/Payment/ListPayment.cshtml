@model List<ListPaymentViewModel>

@{
    ViewData["Title"] = "ListPaymet";
}

<h2>ListPaymetView</h2>


<table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th class="th-sm" scope="col">From Bank Account</th>
            <th class="th-sm" scope="col">To Bank Account</th>
            <th class="th-sm" scope="col">Amount</th>
            <th class="th-sm" scope="col">Reason</th>
            <th class="th-sm" scope="col">Status</th>
            <th class="th-sm" scope="col">Created On</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var p in Model)
        {

            <tr>
                <td>@p.FromBankAccountId</td>
                <td>@p.ToBankAccount</td>
                <td>@p.Amount</td>
                <td>@p.Reason</td>
                <td>@p.Status</td>
                <td>@p.CreatedOn</td>
            </tr>
        }
    </tbody>
    <tfoot>
        <tr>
            <th class="th-sm" scope="col">From Bank Account</th>
            <th class="th-sm" scope="col">To Bank Account</th>
            <th class="th-sm" scope="col">Amount</th>
            <th class="th-sm" scope="col">Reason</th>
            <th class="th-sm" scope="col">Status</th>
            <th class="th-sm" scope="col">Created On</th>
        </tr>
    </tfoot>
</table>

@section Scripts{
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"></script>
    <script src="https://cdn.datatables.net/1.10.19/css/dataTables.bootstrap.min.css"></script>
    <script src="https://code.jquery.com/jquery-3.3.1.js"></script>
    <script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>"
    <script src="https://cdn.datatables.net/1.10.19/js/dataTables.bootstrap.min.js"></script>
    <script>
        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>

}
